//prime number 
#include <stdio.h>

int main()
{
    int i,n,count=0;
    printf("***Prime Number***\n");
    printf("enter a number:");
    scanf("%d", &n);
    
    for(i=1;i<=n;i++){
        if(n%i==0)
            count++;
    }
    
    if(count==2)
        printf("prime number");
    else
        printf("not prime");

    return 0;
}

