//reading elements into array
#include <stdio.h>

int main()
{
    int arr[100];
    int n;
    printf("enter the size of the array: ");
    scanf("%d",&n);
    printf("enter the elements of the array: ");
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    printf("the elements of the array are : ");
    for(int i = 0;i<n;i++){
        printf("%d ",arr[i]);
    }
    

    return 0;
}

