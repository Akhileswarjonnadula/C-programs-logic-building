#include <stdio.h>

int main()
{
    int rollNo[10];
    
    //taking input
    printf("enter the elements of the array:");
    for(int i=0;i<10;i++)
        scanf("%d", &rollNo[i]);
    
    
    //printing
    printf("\n the elements of the array:");
    for(int i=0;i<10;i++)
        printf("%d ", rollNo[i]);  
    
    
    return 0;
}

