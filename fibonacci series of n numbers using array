//fibonacci series of n numbers using array
#include<stdio.h>
void main()
{

  int n;
  int arr[n];
  printf("enter no.of terms: ");
  scanf("%d",&n);
  arr[0]=0,arr[1]=1;
  printf("fibonacci series:\t");
  
  for(int i = 2;i<n;i++)
  {
      arr[i]=arr[i-1]+arr[i-2];
  }
  
  for(int i =0;i<n;i++){
      printf("%d ",arr[i]);
  }
  
}


