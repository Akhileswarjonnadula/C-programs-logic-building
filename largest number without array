//largest number without array
#include <stdio.h>

int main()
{
    int n,ele,large=0;
    printf("enter the no. of elements: ");
    scanf("%d",&n);
    printf("enter the eletments:");
    
    for(int i=1;i<=n;i++){
        scanf("%d",&ele);
        if(ele>large){
            large=ele;
        }
    }
    printf("the largest number is : %d",large);

    return 0;
}

