//factors of a number
#include <stdio.h>

int main()
{
    int i,num;
    printf("enter a number(positive number) to find factors:");
    scanf("%d",&num);
    printf("factors of %d is :",num);
    for(i=1;i<=num;i++){
        if(num%i==0)
           printf("%d ",i);
           
    }
    return 0;
}

