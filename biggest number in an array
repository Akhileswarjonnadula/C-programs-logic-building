//biggest number in an array
#include <stdio.h>

int main()
{
    /*int n;
    int a[n];
    printf("enter no of elements: ");
    scanf("%d",&n);
    printf("enter the elements:");
    for(int i=0;i<n;i++){
        scanf("%d",&a[i]);
    }*/                      //for reading elements  from user
    
    int arr[6]={7,18,3,24,76,11};
    int large = arr[0];
    for(int i=1;i<6;i++)
    {
        if(arr[i]>large){
            large=arr[i];
        }
    }
    printf("the biggest number is %d",large);
    
    return 0;
}

