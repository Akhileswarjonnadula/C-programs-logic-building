//every element in array is repeated twice except one element what is it
#include <stdio.h>

int main()
{
    int a[] = {1,2,3,3,2,1,4,5,6,6,5};
    int res = 0;
    for(int i = 0; i<11; i++){
        res = res ^ a[i];
    }
    printf("the element which is only once in array is: %d", res);

    return 0;
}

