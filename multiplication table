//multiplication table
#include <stdio.h>

int main()
{
    int n,i,result=1;
    printf("**Multiplication Table**\n");
    printf("enter the number for multiplication table: ");
    scanf("%d",&n);
    
    for(i=1;i<=10;i++){
        result = n*i;
        printf("%d * %d = %d \n", n, i, result);
    }

    return 0;
}

